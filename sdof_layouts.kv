#:kivy 2.0.0

#:import Factory kivy.factory.Factory

<CustomDropdown@DropDown>:
    id: dropdown
    on_select:
        app.root.ids.btn.text = '{}'.format(args[1])
        self.dismiss()

    Button:
        id: btn1
        text: 'pin-pin'
        size_hint_y: None
        height: '29dp'
        on_release:
            dropdown.select(btn1.text)

    Button:
        id: btn2
        text: 'fixed-pin'
        size_hint_y: None
        height: '29dp'
        on_release:
            dropdown.select(btn2.text)

    Button:
        id: btn3
        text: 'fixed-fixed'
        size_hint_y: None
        height: '29dp'
        on_release:
            dropdown.select(btn3.text)

    Button:
        id: btn4
        text: 'fixed-free'
        size_hint_y: None
        height: '29dp'
        on_release:
            dropdown.select(btn4.text)




